<?xml version='1.0'?>
<STORE>
	<DRIVER>org.postgresql.Driver</DRIVER>
    <URL>jdbc:postgresql://localhost:5432/eros</URL>
    <NAME>eros</NAME>
    <USER>eros</USER>
    <PASSWORD>erospassword</PASSWORD>
    <TABLE>eros_error</TABLE>
    <COLUMNS>
    	<column columnname="time_stamp" paramname="time_stamp"/>
    	<column columnname="application" paramname="application"/>
    	<column columnname="message" paramname="message"/>
    	<column columnname="instancekey" paramname="inst_id"/>
    	<column columnname="trace_level" paramname="error_level"/>
    	<column columnname="stacktrace" paramname="stacktrace"/>
        <column columnname="arguments" paramname="arguments"/>
       	<column columnname="jinigroup" paramname="group"/>	    		    	
    	<column columnname="host" paramname="host"/>      
    	<column columnname="classname" paramname="classname"/>
	<column columnname="methodname" paramname="methodname"/> 	
	<column columnname="exceptionname" paramname="exceptionname"/> 	
 	</COLUMNS>
</STORE>
