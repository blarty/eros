    <URL>jdbc:twtds:sqlserver://localhost/erosdb</URL>
    <NAME>eros</NAME>
    <USER>eros</USER>
    <PASSWORD>eros</PASSWORD>
    <TABLE>ERROR_STORE</TABLE>
